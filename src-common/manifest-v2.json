{
    "name": "Imageshare Search",
    "description": "An extension that searches Imageshare keywords for matches with users' current site content. Notifies user of matches and opens Imageshare, in a net window, to results view.",
    "version": "0.1.0",
    "background": {
      "scripts" : ["background.js"]
    },
    "browser_action": {
      "default_icon": "screenshot.jpg",
      "default_popup": "popup.html"
    },
    "options_ui": {
      "page": "options.html"
    },
    "permissions": [
      "contextMenus",
      "notifications",
      "storage",
      "activetab",
      "tabs",
      "*://*/*"
    ],
    "content_scripts": [
      {
        "js": ["index.js"],
        "matches": ["*://*/*"]
      }
    ],
    "icons":{
      "16": "screenshot.jpg",
      "128": "screenshot.jpg"
      },
  
    "manifest_version": 2
  }